<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ClinicalTrials.Apps.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ClinicalTrials.Apps"
    Shell.FlyoutBehavior="Disabled"
    Title="Clinical Trials FYI">

    <ShellContent
        Title="Queries"
        ContentTemplate="{DataTemplate local:Queries}"
        Route="queries"/>
    <ShellContent 
        Title="Query" 
        Route="query"
        ContentTemplate="{DataTemplate local:Query}" />
    <ShellContent 
        Title="ClinicalTrials.gov" 
        Route="webview"
        ContentTemplate="{DataTemplate local:WebView}" />
</Shell>
